= simple_form_for(@sie_export, wrapper: :horizontal_form, html: {class: 'form-horizontal'}) do |f|
  = f.error_notification
  = f.input :accounting_period_id, as: :select, collection: @accounting_periods, include_blank: false
  = f.input :sie_type, as: :select, collection: SieExport::SIE_TYPES, include_blank: false

  .form-button-group
    .actions
      = f.submit class: 'btn btn-primary'
