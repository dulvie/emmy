= simple_form_for([@tax_table, @tax_table_row], wrapper: :horizontal_form, html: {class: 'form-horizontal'}) do |f|
  = f.error_notification
  = f.input :calculation
  = f.input :from_wage
  = f.input :to_wage
  = f.input :column_1
  = f.input :column_2
  = f.input :column_3
  = f.input :column_4
  = f.input :column_5
  = f.input :column_6

  .form-button-group
    .actions
      = f.submit class: 'btn btn-primary'
